<div class="container">
  <h4>新規登録</h4>
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "public/shared/error_messages", resource: resource %>

   <div class="row">
    <div class="form-group col-md-3">
<label for="name">氏名</label></div>
<p>(姓)</p>
 <div class="form-group col-md-3">
<%= f.text_field :first_name %></div>
<p>(名)</p>
 <div class="form-group col-md-3">
<%= f.text_field :last_name %></div>
</div>
  <div class="row">
    <div class="form-group col-md-3">
<label for="name">フリガナ</label></div>
<p>(セイ)</p>
 <div class="form-group col-md-3">
<%= f.text_field :first_name_kana %></div>
<p>(メイ)</p>
 <div class="form-group col-md-3">
<%= f.text_field :last_name_kana %></div>
</div>
<div class="row">
    <div class="form-group col-md-3">
<label for="name">郵便番号</label></div>
 <div class="form-group col-md-3">
<%= f.number_field :postal_code %></div>
</div>
<div class="row">
    <div class="form-group col-md-3">
<label for="adress">住所</label></div>
 <div class="form-group col-md-5">
<%= f.text_field :adress %></div>
</div>
<div class="row">
    <div class="form-group col-md-3">
<label for="telephone_number">電話番号</label></div>
 <div class="form-group col-md-3">
<%= f.text_field :telephone_number %></div>
</div>

<div class="row">
    <div class="form-group col-md-3">
<label for="email">メールアドレス</label></div>
 <div class="form-group col-md-3">
<%= f.email_field :email %></div>
</div>

<div class="row">
    <div class="form-group col-md-3">
<label for="pasword">パスワード(6文字以上)</label></div>
 <div class="form-group col-md-3">
    <% f.label :password %>
    <% if @minimum_password_length %>
    <% end %>
    <%= f.password_field :password, autocomplete: "new-password" %></div>
</div>

<div class="row">
    <div class="form-group col-md-3">
<label for="pasword">パスワード(確認用)</label></div>
 <div class="form-group col-md-3">
    <% f.label :password_confirmation %>
    <%= f.password_field :password_confirmation, autocomplete: "new-password" %></div>
</div>
  <div class="row">
    <div class="form-group col-md-2"></div>
    <div class="form-group text-center">
<%= f.submit '新規登録', class: "btn btn-success btn-sm" %></div>
</div>

<% end %>
 <%= link_to "登録済みの方はこちら", new_customer_session_path %>
</div>